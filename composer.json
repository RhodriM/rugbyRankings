{
    "name": "rugbyRankings",
    "description": "World Rugby Rankings calculator",
    "keywords": [
        "rugby",
        "rankings"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Rhodri Morris",
            "email": "author_email@example.com",
            "homepage": "http://author-website.example.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.0.*",
        "squizlabs/php_codesniffer" : "*",
        "phpmd/phpmd" : "@stable",
        "phpdocumentor/phpdocumentor": "2.*",
        "sebastian/phpcpd": "*"
    },
    "autoload": {
        "psr-4": {
            "RugbyRankings\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RugbyRankings\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
